{
    "name": "lukeraymonddowning/nightguard",
    "description": "Set up Laravel Eloquent guards in seconds",
    "type": "library",
    "require": {
        "illuminate/support": "^8.0",
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-expectations": "^1.0",
        "spatie/laravel-ray": "^1.16",
        "orchestra/testbench": "^6.13",
        "laravel/sanctum": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Lukeraymonddowning\\Nightguard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lukeraymonddowning\\Nightguard\\Tests\\": "tests",
            "App\\": "mocks/app"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "luke",
            "email": "lukeraymonddowning@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Lukeraymonddowning\\Nightguard\\Providers\\NightguardServiceProvider"
            ],
            "aliases": {
                "Nightguard": "Lukeraymonddowning\\Nightguard\\Facades\\Nightguard"
            }
        }
    }
}
